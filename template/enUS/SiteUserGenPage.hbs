			{{#eq siteUserCount int1}}
				<button
					class="w3-btn w3-round w3-border w3-border-black w3-ripple w3-padding w3-gray "
						id="refreshThisSiteUserGenPage"
						onclick="patchSiteUserVals( [ {name: 'fq', value: 'pk:' + " + siteRequest_.getRequestPk() + " } ], {}, function() { addGlow($('#refreshThisSiteUserGenPage')); }, function() { addError($('#refreshThisSiteUserGenPage')); }); return false; ">
						<i class="fas fa-sync-alt "></i>
					refresh this site user
				</button>
			{{/eq}}
		{{/ifContainsAnyRoles}}
		{{#ifContainsAnyRoles roles authRolesAdmin}}
			<button
				class="w3-btn w3-round w3-border w3-border-black w3-ripple w3-padding w3-gray "
				onclick="$('#patchSiteUserModal').show(); "
				>
				<i class="fas fa-edit "></i>
				Modify site users
			</button>
			<div id="patchSiteUserModal " class="w3-modal w3-padding-32">
				<div class="w3-modal-content ">
					<div class="w3-card-4 ">
						<header class="w3-container w3-gray">
							<span class="w3-button w3-display-topright " onclick="$('#patchSiteUserModal').hide(); ">×</span>
							<h2 class="w3-padding ">Modify site users</h2>
						</header>
						<div class="w3-container " id=patchSiteUserFormValues>
							{{> "htmFormPATCHSiteUser" o=o}}
							<button
								class="w3-btn w3-round w3-border w3-border-black w3-ripple w3-padding w3-margin w3-gray "
								onclick="patchSiteUser(null, $('#patchSiteUserFormValues'), ", Optional.ofNullable(siteUser_).map(SiteUser::getPk).map(a -> a.toString()).orElse("null", ", function() {}, function() {}); ")
								>Modify site users</button>
						</div>
					</div>
				</div>
			</div>
			<button
				class="w3-btn w3-round w3-border w3-border-black w3-ripple w3-padding w3-gray "
				onclick="$('#postSiteUserModal').show(); "
				>
				<i class="fas fa-file-plus "></i>
				Create a site user
			</button>
			<div id="postSiteUserModal " class="w3-modal w3-padding-32">
				<div class="w3-modal-content ">
					<div class="w3-card-4 ">
						<header class="w3-container w3-gray">
							<span class="w3-button w3-display-topright " onclick="$('#postSiteUserModal').hide(); ">×</span>
							<h2 class="w3-padding ">Create a site user</h2>
						</header>
						<div class="w3-container " id=postSiteUserFormValues>
							<div id="postSiteUserForm">
								{{> "htmFormPOSTSiteUser" o}}
							</div>
							<button
								class="w3-btn w3-round w3-border w3-border-black w3-ripple w3-padding w3-margin w3-gray "
								onclick="postSiteUser($('#postSiteUserForm')); "
								>Create a site user</button>
						</div>
					</div>
				</div>
			</div>
		{{/ifContainsAnyRoles}}
