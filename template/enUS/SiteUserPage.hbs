{{#partial "htmlUrlSiteUser"}}
/api/user?q={{query.q}}&rows={{#if rows}}{{rows}}{{else}}{{pagination.rows}}{{/eq}}&start={{#if start}}{{start}}{{else}}{{pagination.start}}{{/eq}}{{#each query.fq}}{{#eq fq this}}{{else}}&fq={{fq}}:{{val}}{{/eq}}{{/each}}{{#each query.sort}}{{#eq sort this}}{{else}}&sort={{var}} {{order}}{{/eq}}{{/each}}{{/partial}}

{{#partial "htmlBodySiteUserGenPage"}}

		{{#if listSiteUser_}}
			{{#eq listSiteUser_.length 1}}
				{{#eq params.query.q "*:*"}}
					{{#with listSiteUser_[0]}}
					{{#if pageH1}}
						<h1>
							<a href="{{> htmlUrlSiteUser}}" class="w3-bar-item w3-btn w3-center w3-block w3-gray w3-hover-gray">
								{{#if contextIconCssClasses}}
									<i class="contextIconCssClasses site-menu-icon "></i>
								{{/if}}
								<span class="">{{pageH1}}</span>
							</a>
						</h1>
					{{/if}}
					<div class="w3-padding-16 w3-card-4 w3-light-grey ">
					{{#if pageH2}}
						<h2>
							<span class"w3-bar-item w3-padding w3-center w3-block w3-gray">
								<span class="">{{pageH2}}</span>
							</span>
						</h2>
					{{/if}}
					{{#if pageH3}}
						<h3>
							<span class="w3-bar-item w3-padding w3-center w3-block w3-gray">
								<span class="">{{pageH3}}</span>
							</span>
						</h3>
					{{/if}}
				{{else}}

					<h1>
						<a href="{{> htmlUrlSiteUser}}" class="w3-bar-item w3-btn w3-center w3-block w3-gray w3-hover-gray">
							{{#if contextIconCssClasses}}
								<i class="contextIconCssClasses + " site-menu-icon "></i>
							{{/if}}
							<span class="">{{pageH1}}</span>
						</a>
					</h1>
					<div class="">
						<div>

							{{#if pagination.pagePrevious}}
								<a href="{{> htmlUrlSiteUser fq=start}}">
									<i class="fas fa-arrow-square-left "></i>
								</a>
								{{else}}
								<i class="fas fa-arrow-square-left w3-opacity "></i>
							{{/if}}

							if(rows1 <= 1) {
								<i class="fas fa-minus-square w3-opacity "></i>
							{{else}}
								<a.a("href", "/api/user?q=", query, fqs, sorts, "&start=", start1, "&rows=", rows2)>;
									<i class="fas fa-minus-square "></i>
								</a>
							{{/if}}

							<a.a("href", "/api/user?q=", query, fqs, sorts, "&start=", start1, "&rows=", rows3)>;
								<i class="fas fa-plus-square "></i>
							</a>

							{{#gte start3 num}}
								<i class="fas fa-arrow-square-right w3-opacity "></i>
							{{else}}
								<a.a("href", "/api/user?q=", query, fqs, sorts, "&start=", start3, "&rows=", rows1)>;
									<i class="fas fa-arrow-square-right "></i>
								</a>
							{{/gte}}
							<span>(start1 + 1), " - ", (start1 + rows1), " of ", num</span>
						</div>
						{{#block "table1SiteUserGenPage"}}
					</div>
				{{/eq}}
			{{else}}
				<h1>
					<a href="{{> htmlUrlSiteUser}}" class="w3-bar-item w3-btn w3-center w3-block w3-gray w3-hover-gray">
						{{#if contextIconCssClasses}}
							<i class="contextIconCssClasses + " site-menu-icon "></i>
						{{/if}}
						<span class="">{{pageH1}}</span>
					</a>
				</h1>
				<div class="">
					<div>

						{{#if pagination.pagePrevious}}
							<a href="{{> htmlUrlSiteUser fq=start}}">
								<i class="fas fa-arrow-square-left "></i>
							</a>
							{{else}}
							<i class="fas fa-arrow-square-left w3-opacity "></i>
						{{/if}}

						if(rows1 <= 1) {
							<i class="fas fa-minus-square w3-opacity "></i>
						{{else}}
							<a.a("href", "/api/user?q=", query, fqs, sorts, "&start=", start1, "&rows=", rows2)>;
								<i class="fas fa-minus-square "></i>
							</a>
						{{/if}}

						<a.a("href", "/api/user?q=", query, fqs, sorts, "&start=", start1, "&rows=", rows3)>;
							<i class="fas fa-plus-square "></i>
						</a>

						{{#gte start3 num}}
							<i class="fas fa-arrow-square-right w3-opacity "></i>
